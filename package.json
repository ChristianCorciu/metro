{
  "name": "dernier-metro-api",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:integration": "jest integration.test.js --runInBand --detectOpenHandles --forceExit" ,   "test:watch": "jest --watch --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/tests/**/*.test.js"],
    "verbose": true,
    "clearMocks": true,
    "testTimeout": 10000
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}